*{margin:0;padding:0}.activity-body{background-color:#d92d32;overflow:scroll;-webkit-overflow-scrolling:touch}.activity-header{height:5.68rem;background:url(../images/hrader.png) no-repeat center 0;background-size:100%}.activity{padding:0 0 .53333rem}.activity .section{background-color:#fae9d9;padding:0;margin:0 .53333rem}.activity .section~.section{margin-top:.53333rem}.section-header{padding:.45333rem .64rem;font-size:.42667rem;color:#8d561c;background-color:#fbdbbb}.section-main{padding:0 .53333rem .26667rem}.section-main .top{font-size:.42667rem;color:rgba(169,111,28,.5);padding:.37333rem 0;border-bottom:1px solid #efcda7}.section-main .cotent{color:#a96f1c;font-size:.42667rem;text-indent:2em;padding:.26667rem 0;line-height:1.6}.section-main .cotent:after{content:'';height:.8rem;background:url(../images/icon2.png) no-repeat 100%;background-size:.42667rem;display:block}.section-main .cotent:before{content:'';background:url(../images/icon1.png) no-repeat 100%;background-position:0;background-size:.42667rem;height:.8rem;display:block}