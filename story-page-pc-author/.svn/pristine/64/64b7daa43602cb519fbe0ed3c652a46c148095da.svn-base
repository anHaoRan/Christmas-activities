@import 'common/fun';
.setroll{
  width: 833px;
  height: auto;
}
.addbtn-box{
  overflow: hidden;
  margin: 30px 0 17px 0;
}
.sr-t{
  span{
    &:nth-child(1){
      width: 84px;
    }
    &:nth-child(2){
      width: 333px;
    }
    &:nth-child(3){
      width: 127px;
    }
    &:nth-child(4){
      width: 127px;
    }
    &:nth-child(5){
      width: 160px;
    }
  }
}
.setroll-list{
  list-style: none;
  margin: 0;
  padding: 0;
  li{
    height: 35px;
    border-bottom: 1px dotted #cbcbcb;
    &>span{
      display: inline-block;
      float: left;
      text-align: center;
      line-height: 35px;
      &.num{
        width: 86px;
      }
      &.rollname{
        width: 333px;
      }
      &.sectionnum{
        width: 86px;
      }
      &.wordsnum{
        width: 168px;
      }
      &.operate{
        width: 160px;
        a{
          display: inline-block;
          float: left;
          width: 80px;
          text-align: center;
          &.amend{
            color: #499fff;
          }
          &.del{
            color: #ff3a3a;
          }
        }
      }
    }
  }
}