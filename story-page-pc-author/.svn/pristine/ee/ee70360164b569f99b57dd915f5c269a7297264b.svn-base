.popup,body,html{width:100%;height:100%}.popup{background:rgba(0,0,0,.5);position:fixed;left:0;top:0}.popup h3,.popup input[type=text],.popup p{margin:0;padding:0}.popup .popup-box{width:696px;height:368px;background-color:#fff;padding:35px;position:absolute;top:50%;left:50%;margin-top:-184px;margin-left:-348px}.popup .popup-box .popup-close{width:25px;height:25px;border-radius:20px;background-color:#f3f3f3;display:block;float:right;margin-right:-46px;margin-top:-46px;background-image:url(../images/Close.png);background-size:8px;background-repeat:no-repeat;background-position:50%}.popup .popup-body,.popup .popup-title{padding-left:20px}.popup .popup-title{line-height:40px;background-color:#f6f6f6;border-top:1px solid #eaeaea;margin-bottom:32px;font-size:15px;color:#5a5a5a}.popup .popup-body{height:190px}.popup .popup-body .popup-name{margin-right:15px;line-height:34px;display:inline-block}.popup .popup-body .popup-v{width:406px;line-height:34px;text-indent:14px;border:1px solid #e1e1e1;border-radius:3px}.popup .popup-body .popup-t{margin-top:14px;margin-left:46px;color:#b1b1b1}.popup .popup-footer{text-align:right}.popup .popup-footer .btn{padding:0 28px}