.drafts{width:833px;height:auto}.dr-t{margin-top:36px}.dr-t span:nth-child(1){width:86px}.dr-t span:nth-child(2){width:333px}.dr-t span:nth-child(3){width:86px}.dr-t span:nth-child(4){width:168px}.dr-t span:nth-child(5){width:160px}.drafts-list{list-style:none;margin:0;padding:0}.drafts-list li{height:35px;border-bottom:1px dotted #cbcbcb}.drafts-list li>span{display:inline-block;float:left;text-align:center;line-height:35px}.drafts-list li>span.num{width:86px}.drafts-list li>span.name{width:333px}.drafts-list li>span.wordsnum{width:86px}.drafts-list li>span.time{width:168px}.drafts-list li>span.operate{width:160px}.drafts-list li>span.operate a{display:inline-block;float:left;width:80px;text-align:center}.drafts-list li>span.operate a.amend{color:#499fff}.drafts-list li>span.operate a.del{color:#ff3a3a}