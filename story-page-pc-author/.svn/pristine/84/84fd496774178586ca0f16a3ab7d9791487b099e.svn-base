:focus {
  outline: none;
}



.header {
  height: 125px;
  background: #1e2539;
  margin-bottom: 50px;
  min-width: 1400px;
  .slogan {
    margin-top: 34px;
    float: left;
    
    img {
      width: 213px;
      height: 57px;
    }
  }
  .user{
    display: inline-block;
    float: right;
    line-height: 57px;
    margin-top: 34px;
    padding-right: 20px;
    a{
      background: transparent;
      border-color: transparent;
      color: red;
      cursor: pointer;
      &:hover{
        text-decoration: underline;
      }
    }
  }
}