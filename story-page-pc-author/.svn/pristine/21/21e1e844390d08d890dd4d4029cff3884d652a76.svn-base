.boonbody .boonmain div.details {
  width: 786px;
  height: 100%;
  background: url("../images/boon/detailsbg.png") no-repeat;
  float: right;
  padding: 0 50px 0 106px;
}
.boonbody .boonmain div.detailsbig {
  width: 786px;
  height: 100%;
  background: url("../images/boon/detailsbig.png") no-repeat;
  float: right;
  padding: 0 50px 0 106px;
  background-size: 100% 100%;
}
.boonbody .boonmain div.details>div {
  height: 0;
  overflow: hidden;
}
.boonbody .boonmain div.details>div.active{
  height: auto;
}
.boonbody .boonmain div.details dl {
  margin-top: 48px;
}
.boonbody .boonmain div.details .specialDl {
  margin: 0;
  font-size: 10px;
}
.boonbody .boonmain div.details dt {
  height: 32px;
  font-size: 24px;
  color: #fdb400;
  padding-left: 52px;
  position: relative;
}
.boonbody .boonmain div.details dt .moon {
  display: inline-block;
  position: absolute;
  left: 0;
  width: 32px;
  height: 32px;
  background: url("../images/boon/moon.png") no-repeat 100% 100%;
}
.boonbody .boonmain div.details dt span {
  display: inline-block;
  float: left;
  height: 32px;
}
.boonbody .boonmain div.details dd {
  padding-left: 52px;
  font-size: 14px;
  line-height: 32px;
  color: #ffe792;
}
.boonbody .boonmain div.details dd h1 {
  font-size: 14px;
  display: inline-block;
  width: 110px;
  line-height: 32px;
  margin: 0;
  margin-left: 20px;
}
.boonbody .boonmain div.details .annotation {
  color: #7e484b;
  font-size: 14px;
}
