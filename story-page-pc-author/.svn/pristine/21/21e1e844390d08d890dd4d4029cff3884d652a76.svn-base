*{
  box-sizing: border-box;
}
.boonbody {
  min-width: 1228px;
  height: 1945px;
  overflow: hidden;
  padding-top: 40px;
  background: url("../images/boon/boonbg.jpg") no-repeat;
  background-size: 100% 100%;
}
.boonbody .logo {
  width: 1100px;
  height: 60px;
  margin: 0 auto;
}
.boonbody .logo img {
  width: 200px;
  height: 41px;
  cursor: pointer;
}
.boonbody .banner {
  width: 1228px;
  height: 602px;
  margin: 0 auto;
  background: url("../images/boon/marslog.png") no-repeat 100% 100%;
}
.boonbody .boonmain {
  width: 1000px;
  height: 1398px;
  margin: 0 auto;
  margin-top: -95px;
  position: relative;
}
.boonbody .boonmain .broadside {
  width: 260px;
  height: 966px;
  position: absolute;
  top: -33px;
  background: url("../images/boon/broadsidebg.png") no-repeat 100% 100%;
  padding-top: 49px;
}
.boonbody .boonmain .broadside dl.active dd{
  height: auto;
  width: 240px;
}
.boonbody .boonmain .broadside dl.active dt i{
  background: url("../images/boon/bot-tri.png") no-repeat 100% 100%;
}
.boonbody .boonmain .broadside dl.noStyle {
  overflow: hidden;
}
.boonbody .boonmain .broadside dl.noStyle dd {
  background: none;
}
.boonbody .boonmain .broadside dl {
  margin: 0 auto;
  min-height: 70px;
}
.boonbody .boonmain .broadside dl.noStyle.active dt {
  width: 216px;
  height: 70px;
  margin: 0 auto;
  font-size: 19px;
  text-align: center;
  line-height: 70px;
  color: #46032c;
  cursor: pointer;
  background: url("../images/boon/highbg.png") no-repeat;
  background-size: 100% 100%;
}
.boonbody .boonmain .broadside dl dt.checked {
  width: 216px;
  height: 70px;
  margin: 0 auto;
  font-size: 19px;
  text-align: center;
  line-height: 70px;
  color: #46032c;
  cursor: pointer;
  background: url("../images/boon/highbg.png") no-repeat;
  background-size: 100% 100%;
}
.boonbody .boonmain .broadside dl dt {
  width: 216px;
  height: 70px;
  margin: 0 auto;
  font-size: 19px;
  text-align: center;
  line-height: 70px;
  color: #fdb400;
  position: relative;
  cursor: pointer;
}
.boonbody .boonmain .broadside dl dt i {
  width: 16px;
  height: 16px;
  position: absolute;
  right: 13px;
  top: 28px;
}
.boonbody .boonmain .broadside dl dt .right-tri {
  background: url("../images/boon/right-tri.png") no-repeat 100% 100%;
}
.boonbody .boonmain .broadside dl dt .bot-tri {
  background: url("../images/boon/bot-tri.png") no-repeat 100% 100%;
}
.boonbody .boonmain .broadside dl dd {
  margin: 0 auto;
  padding-bottom: 10px;
  background: url("../images/boon/yellowS.jpg") no-repeat 100% 100%;
  height: 0;
  width: 0;
  overflow: hidden;
}
.boonbody .boonmain .broadside dl dd em {
  font-size: 16px;
  display: block;
  width: 216px;
  height: 54px;
  margin: 8px auto;
  list-style: none;
  text-align: center;
  line-height: 54px;
  color: #fdb400;
  cursor: pointer;
  font-style: normal;
}
.boonbody .boonmain .broadside dl dd em.optS {
  background: url("../images/boon/highbg.png") no-repeat 100% 100%;
  color: #46032c;
}
.boonbody .boonmain .broadside dl dd em.optS:hover {
  background: url("../images/boon/highbg.png") no-repeat 100% 100%;
  color: #46032c;
}
.boonbody .boonmain .broadside dl dd .hoverS {
  background: #5f102a;
  border-radius: 6px;
  color: #fff;
}
.boonbody .boonmain .broadside dl dd em:hover {
  background: #5f102a;
  border-radius: 6px;
  color: #fff;
}

