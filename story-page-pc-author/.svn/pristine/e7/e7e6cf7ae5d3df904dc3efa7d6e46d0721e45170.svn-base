.worksList{
  width: 1140px;
  height: auto;
}
.worksList-header{
  height: 54px;
  padding-top: 10px;
  .hint{
    margin: 0;
    line-height: 34px;
    span{
      color: #abb1b7;
    }
    .num{
      font-style: normal;
      color: #7798b1;
      padding: 0 5px;
    }
  }
}
.wl-t{
  span{
    &:nth-child(1){
      width: 286px;
    }
    &:nth-child(2){
      width: 285px;
    }
    &:nth-child(3){
      width: 158px;
    }
    &:nth-child(4){
      width: 207px;
    }
    &:nth-child(5){
      width: 204px;
    }
  }
}