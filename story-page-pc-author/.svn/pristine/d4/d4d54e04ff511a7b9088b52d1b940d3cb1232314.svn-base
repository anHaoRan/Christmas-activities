@import 'common/fun';
@import 'common/variable';
.money-color {
    color: #95591e;
}

.money-color-red {
    color: #ff3a3a;
}

ul {
    list-style: none;
    padding: 0px;
    margin: 0px;
}

.content-r {
    width: 1140px;
    margin-bottom: 100px;
}

.main {
    width: 934px;
}

.content-r-header {
    padding-left: 15px;
    background: #fbecc8;
    overflow-x: hidden;
    border-radius: 5px;
    margin-top: 20px;
    .item {
        float: left;
        line-height: 34px;
        margin-left: 40px;
        &:nth-of-type(1) {
            margin-left: 0px;
        }
        span {
            line-height: 14px;
        }
    }
}

.reward-filter {
    margin-top: 40px;
    .form-group {
        &::after {
            width: 10px;
            height: 5px;
            content: "";
            background: url(../images/read-arrow.png);
            background-size: 10px auto;
            position: absolute;
            background-position: 0px -5px;
            right: 10px;
            top: 15px;
        }
    }
    .reward-book {
        margin-right: 90px;
        margin-left: 13px;
    }
}

.content-r-all {
    line-height: 40px;
    text-align: right;
    margin-top: 8px;
    color: #a5acb2;
}

.order-result-t {
    span {
        text-overflow: ellipsis;
        overflow: hidden;
        white-space: nowrap;
        padding: 0 10px;
        &:nth-of-type(1) {
            width: 124px;
        }
        &:nth-of-type(2) {
            width: 200px;
        }
        &:nth-of-type(3) {
            width: 196px;
        }
        &:nth-of-type(4) {
            width: 220px;
        }
        &:nth-of-type(5) {
            width: 192px;
        }
    }
}

.content-r-result {
    .order-list {
        .list-item {
            height: 34px;
            border-bottom: 1px #cbcbcb dotted;
            clear: both;
            span {
                float: left;
                text-align: center;
                line-height: 34px;
                overflow: hidden;
                text-overflow: ellipsis;
                white-space: nowrap;
                padding: 0 10px;
                &:nth-of-type(1) {
                    width: 124px;
                }
                &:nth-of-type(2) {
                    width: 200px;
                }
                &:nth-of-type(3) {
                    width: 196px;
                }
                &:nth-of-type(4) {
                    width: 220px;
                }
                &:nth-of-type(5) {
                    width: 192px;
                }
            }
        }
    }
}

.result-t {
    span {
        text-overflow: ellipsis;
        overflow: hidden;
        white-space: nowrap;
        padding: 0 10px;
        &:nth-of-type(1) {
            width: 174px;
        }
        &:nth-of-type(2) {
            width: 274px;
        }
        &:nth-of-type(3) {
            width: 294px;
        }
        &:nth-of-type(4) {
            width: 192px;
        }
    }
}

.content-r-result {
    color: #4e4e4e;
    .list {
        .list-item {
            height: 34px;
            border-bottom: 1px #cbcbcb dotted;
            clear: both;
            span {
                float: left;
                text-align: center;
                line-height: 34px;
                text-overflow: ellipsis;
                overflow: hidden;
                white-space: nowrap;
                padding: 0 10px;
                &:nth-of-type(1) {
                    width: 174px;
                }
                &:nth-of-type(2) {
                    width: 274px;
                }
                &:nth-of-type(3) {
                    width: 296px;
                }
                &:nth-of-type(4) {
                    text-align: center;
                }
            }
        }
    }
}

.order-list {
    .list-item {
        height: 34px;
        border-bottom: 1px #cbcbcb dotted;
        clear: both;
        span {
            float: left;
            text-align: center;
            line-height: 34px;
            text-overflow: ellipsis;
            overflow: hidden;
            white-space: nowrap;
            padding: 0 10px;
            &:nth-of-type(1) {
                width: 84px;
            }
            &:nth-of-type(2) {
                width: 156px;
            }
            &:nth-of-type(3) {
                width: 196px;
            }
            &:nth-of-type(4) {
                width: 204px;
            }
            &:nth-of-type(5) {
                width: 192px;
            }
        }
    }
}

.screening-results {
    line-height: 52px;
    text-align: right;
    margin-bottom: 17px;
}
