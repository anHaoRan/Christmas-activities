import {
  UPDATE_BOY_HOT_WOMEN,
  UPDATE_BOY_BIG_PUSH,
  UPDATE_BOY_SMALL_PUSH,
  UPDATE_BOY_NEW_BOOK_LIST,
  UPDATE_BOY_BIG_PRODUCTS_RECOMMENDED,
  UPDATE_BOY_SMALL_PRODUCTS_RECOMMENDED,

  UPDATE_BOY_TYPE_ONE,
  UPDATE_BOY_TYPE_TWO,
  UPDATE_BOY_TYPE_THREE,
  UPDATE_BOY_TYPE_FOUR,

  UPDATE_BOY_BANNER,
} from '../constants/ActionTypes';

export function updateBoyBanner(banner) {
  return {
    type: UPDATE_BOY_BANNER,
    payload: banner
  };
}
export function updateBoyTypeOne(list) {
  return {
    type: UPDATE_BOY_TYPE_ONE,
    payload: list
  };
}

export function updateBoyTypeTwo(list) {
  return {
    type: UPDATE_BOY_TYPE_TWO,
    payload: list
  };
}

export function updateBoyTypeThree(list) {
  return {
    type: UPDATE_BOY_TYPE_THREE,
    payload: list
  };
}

export function updateBoyTypeFour(list) {
  return {
    type: UPDATE_BOY_TYPE_FOUR,
    payload: list
  };
}

export function updateBoyBigProducts(list) {
  return {
    type: UPDATE_BOY_BIG_PRODUCTS_RECOMMENDED,
    payload: list
  };
}
export function updateBoySmallProducts(list) {
  return {
    type: UPDATE_BOY_SMALL_PRODUCTS_RECOMMENDED,
    payload: list
  };
}
export function updateBoySmallPushList(list) {
  return {
    type: UPDATE_BOY_SMALL_PUSH,
    payload: list
  };
}
export function updateBoyBigPushList(list) {
  return {
    type: UPDATE_BOY_BIG_PUSH,
    payload: list
  };
}
export function updateBoyHotWomenList(list) {
  return {
    type: UPDATE_BOY_HOT_WOMEN,
    payload: list
  };
}

export function updateBoyNewBookList(list) {
  return {
    type: UPDATE_BOY_NEW_BOOK_LIST,
    payload: list
  };
}

