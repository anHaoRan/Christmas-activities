.fans{width:1000px;height:auto;margin:0 auto;margin-bottom:80px;overflow:hidden}.fans-ranking{width:700px;float:left;height:auto}.fans-ranking>div{height:61px;overflow:hidden}.fans-ranking>div h2{margin:0;padding:0;float:left;font-size:26px;line-height:61px}.fans-ranking>div span{float:left;line-height:64px;font-size:22px;color:#fd2f35}.fans-ranking .ranking-rect{list-style:none;margin:0;padding:0;width:100%;height:auto;padding:10px;padding-bottom:0;border:1px solid #e6e5e5}.fans-ranking li.list-title{height:58px;width:100%;border:1px solid #e8e8e8;overflow:hidden;line-height:66px;font-weight:600;background:#eee;font-size:20px;color:#333;margin-bottom:10px}.fans-ranking li{height:36px;line-height:36px;font-size:14px}.fans-ranking li a{color:#282c42}.fans-ranking li:nth-child(even){background:#f8f7f7}.fans-ranking li span{display:block;float:left;height:100%}.fans-ranking li span.num{position:relative;width:112px;margin-right:128px;text-align:center}.fans-ranking li span.num i{position:absolute;right:25px;top:11px;width:16px;height:13px}.fans-ranking li span i.champion{background:url(../images/th-champion.png) no-repeat}.fans-ranking li span i.runner-up{background:url(../images/th-runner-up.png) no-repeat}.fans-ranking li span i.third{background:url(../images/th-third.png) no-repeat}.fans-ranking li span.name{width:284px}.fans-ranking li span.money{width:150px}.fans-msg{width:275px;float:right;height:297px;padding-top:61px}.fans-msg .back{display:block;width:126px;height:34px;background:#f50e0e;font-size:14px;color:#fff;line-height:34px;text-align:center;border-radius:27px;margin-bottom:20px}.fans-msg dl{margin:0;padding:0;width:275px;height:182px;border:1px solid #e1e1e1;padding:4px}.fans-msg dt{width:100%;height:53px;background:#ffe8ee;font-size:18px;color:#413f3f;line-height:53px;padding-left:13px}.fans-msg dd{margin:0;padding:0;font-size:14px;line-height:21px;padding:0 12px 0 15px;margin-top:13px}