<script src="//c.hotread.com/assets/js/vue-resource.min.js"></script>
<script type="text/javascript">
  Vue.http.interceptors.push(function (request, next) {
    request.url += (request.url.indexOf('?') > 0 ? '&' : '?') + 'cb=' + (new Date() - 0);
    NProgress.start();
    next(function () {
      NProgress.done();
    });
  });

  function to10(num) {
    return num > 9 ? (num + '') : ('0' + num);
  }
  Vue.filter('datetime', function (value) {
    var date = new Date(value);
    return date.getFullYear() + '-' + to10(date.getMonth() + 1) + '-' + to10(date.getDay()) + ' ' + to10(date.getHours()) + ':' + to10(date.getMinutes());
  });

  Vue.filter('clean', function (value) {
    return (value || '').replace(/\n+/g, '').replace(/\s+/g, '');
  });

  Vue.filter('ten', function (value) {
    return to10(value + 1);
  });
  
  var DEFAULT_FACE = '//p2.duyao001.com/image/article/15bcc989e86a8b1deb2dcc7286b23e59_150x150.png';
  Vue.filter('avatar', function (url) {
    if (url && /_\d+x\d+\./.test(url)) {
      url = url.replace(/_\d+x\d+\./, '_150x150.');
    }
    return url || DEFAULT_FACE;
  });
</script>
