{
  "name": "story-node-pc",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "node ./bin/www",
    "try": "node ./bin/www2",
    "start2": "export NODE_ENV=production && node ./bin/www",
    "super": "export NODE_ENV=development && supervisor -w ./src,./server ./bin/www",
    "dev": "rm -rf public/build && webpack --progress --colors --watch --config ./webpack.dev.js",
    "test": "rm -rf public/build && export NODE_ENV=test && webpack --progress --colors --config ./webpack.test.js",
    "pro": "export NODE_ENV=production && webpack --progress --colors -p",
    "babel": "sudo ./node_modules/.bin/babel-node ./bin/www",
    "clean": "rm -rf build && mkdir build && cp -rf try ./build && cp ./config.js ./build && cd build && mkdir server src && cp -rf ../src/layouts ./src/",
    "build-server": "./node_modules/.bin/babel -d ./build/server ./server -s",
    "build-src": "./node_modules/.bin/babel -d ./build/src ./src -s",
    "build-pro": "npm run clean && npm run build-server && npm run build-src"
  },
  "author": "",
  "license": "ISC",
