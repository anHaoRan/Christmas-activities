.catalogue-box{width:1000px;height:auto;margin:0 auto 80px;padding-top:21px}.catalogue-box h2.catalogue-bookName{margin:0;padding:0;height:57px;font-size:26px;line-height:57px;color:#333;font-weight:600}.catalogue-box .catalogue-list{width:100%;height:auto;border:1px solid #e6e5e5;padding:0 10px;padding-bottom:10px}.catalogue-box .catalogue-list dl.roll{margin:0;padding:0;width:100%;height:auto;margin-top:10px}.catalogue-box .catalogue-list dt{position:relative;height:41px;width:100%;background:#f5f5f5;padding-left:24px;cursor:pointer}.catalogue-box .catalogue-list dt span{display:block;height:17px;float:left;width:auto;font-size:16px;font-weight:600;line-height:17px;margin-top:13px}.catalogue-box .catalogue-list dt .rect{display:block;float:left;width:3px;height:17px;background:#ff3131;margin-right:8px}.catalogue-box .catalogue-list dt .ico{font-style:normal;position:absolute;right:25px;top:16px;width:17px;height:7px;background:url(../images/bottom-jt.png) no-repeat}.catalogue-box .catalogue-list dd{margin:0;padding:0;width:100%;height:auto;overflow:hidden;display:none}.catalogue-box .catalogue-list dd a{float:left;width:325px;height:44px;line-height:43px;border-bottom:1px dashed #eee;color:#333;font-size:14px;padding-left:20px}.catalogue-box .catalogue-list dd a:hover{color:#f50e0e}.catalogue-box .catalogue-list dd a.vip:after{margin-left:5px;content:"";font-size:0;width:20px;height:19px;background:url(../images/pay.png) no-repeat 50%/cover;display:inline-block;vertical-align:middle}.catalogue-box .catalogue-list dd a .chapter-name{white-space:nowrap;float:left;max-width:300px;overflow:hidden;text-overflow:ellipsis}.catalogue-box .catalogue-list dl.on dd{display:block}.catalogue-box .catalogue-list dl.on dt .ico{background:url(../images/top-jt.png) no-repeat}