@import "common/variable";
@import "common/fun";

.details-main {
  width: 1000px;
  height: auto;
  margin: 74px auto 90px auto;
  overflow: hidden;
}
// 火力值
.fire-value {
  position: absolute;
  top: 0;
  right: 0;
  // width: 65px;
  height: 50px;
  padding-top: 5px;
  border: 1px solid #fcc776;
  border-radius: 4px;
  color: #fbb03b;

  .fv-count {
    text-align: right;
    padding: 0 5px;
    line-height: 1;
    em {
      font-size: 22px;
      font-style: normal;
    }
  }

  .fv-label {
    text-align: center;
    line-height: 20px;
    padding: 0 5px;
    &::before {
      display: inline-block;
      margin-right: 3px;
      content: "";
      width: 11px;
      height: 17px;
      background: bg('fire.png') no-repeat 50% 50% / 11px 17px;
      vertical-align: -2px;
    }
  }
}


/*详情*/

.details {
  float: left;
  width: 680px;
  height: 100%;
  /*个人简介*/
  /*带图片部分*/
  .intro-sip {
    height: 300px;
    .bookImg {
      display: block;
      float: left;
      width: 184px;
      height: 261px;
      border: 1px solid #e6e5e5;
      overflow: hidden;
    }
    .intro-wz {
      position: relative;
      width: 460px;
      height: 300px;
      float: right;
      .booxName {
        @include marPad();
        font-size: 24px;
        font-weight: 600;
        line-height: 26px;
        color: $textColor;
        overflow: hidden;
        text-overflow: ellipsis;
        white-space: nowrap;
        padding-right: 70px;
      }
      .authorName {
        font-size: 14px;
        font-weight: normal;
        line-height: 32px;
        height: 32px;
        margin-bottom: 3px;
      }
      em.classify,
      em.lab,
      em.worNum {
        font-style: normal;
        display: block;
        height: 25px;
        line-height: 25px;
        font-size: 14px;
        color: #666666;
      }
      p.state {
        @include marPad();
        @include showApo();
        height: 35px;
        width: 100%;
        border: 1px solid #e1e1e1;
        border-radius: 3px;
        background: #f5f5f5;
        color: #666666;
        line-height: 35px;
        font-size: 14px;
        padding-left: 13px;
        margin-top: 13px;
      }
    }
    dl.btn-list {
      @include marPad();
      margin-top: 32px;
    }
    dt {
      float: left;
      a {
        display: block;
        float: left;
        width: 150px;
        height: 45px;
        border-radius: 3px;
        text-align: center;
        line-height: 45px;
        font-size: 18px;
        margin-right: 24px;
        color: $textColor;
        border: 1px solid #fd2f35;
      }
      a:nth-child(2) {
        color: white;
        background: #fd2f35;
      }
    }
    dd {
      @include marPad();
      float: left;
      div {
        position: relative;
        float: left;
        width: 44px;
        height: 44px;
        border-radius: 3px;
        cursor: pointer;
        &::before{
          position: absolute;
          left: 0;
          top: 44px;
          content: '';
          width: 44px;
          height: 10px;
        }
      }
      div.twoCode {
        background: #f5f5f5 bg('phone-two-code.png') no-repeat;
        background-position: 13px 7px;
      }
      span.tCBox{
        display: none;
        position: absolute;
        z-index: 99;
        left: -38px;
        top: 54px;
        width: 120px;
        height: 120px;
        background: #e5e5e5;
        border-radius: 3px;
        &>img{
          display: block;
          width: 108px;
          height: 108px;
          margin: 0 auto;
          margin-top: 6px;
        }
        &::after{
          position: absolute;
          left: 57px;
          top: -8px;
          content: '';
          width: 0;
          height: 0;
          border-left: 4px solid transparent;
          border-right: 4px solid transparent;
          border-bottom: 8px solid #e5e5e5;
        }
      }
      div.transpond {
        background: #f5f5f5 bg('transpond.png') no-repeat;
        background-position: 14px 13px;
        &::before{
          left: -23px;
          top: 18px;
          width: 67px;
          height: 36px;
        }
      }
      span.tBox{
        position: absolute;
        z-index: 99;
        left: -117px;
        top: 54px;
        width: 162px;
        height: 61px;
        background: #e5e5e5;
        border-radius: 3px;
        em{
          display: block;
          width: 39px;
          height: 39px;
          float: right;
          margin: 11px 12px 0 0;
        }
        .qq{
          background: bg('qq.png');
        }
        .weibo{
          background: bg('weibo.png');
        }
        .weixin{
          background: bg('weixin.png');
        }
        &::after{
          position: absolute;
          left: 136px;
          top: -8px;
          content: '';
          width: 0;
          height: 0;
          border-left: 4px solid transparent;
          border-right: 4px solid transparent;
          border-bottom: 8px solid #e5e5e5;
        }
      }
      div.twoCode {
        margin-right: 24px;
        padding-top: 0;
        padding-top: 7px;
      }
    }
  }
  /*文字简介*/
  .intro-con {
    ul {
      list-style: none;
      @include marPad();
      height: 44px;
      width: 100%;
      border-bottom: 1px solid #e6e5e5;
      li {
        float: left;
        height: 43px;
        padding: 0 32px;
        line-height: 43px;
        font-size: 18px;
        color: $textColor;
        cursor: pointer;
      }
      li.on {
        border-bottom: 3px solid #fd2f35;
      }
    }
    .introduce {
      padding-top: 21px;
      padding-bottom: 15px;
      p {
        margin: 0 0 5px;
        text-indent: 2em;
      }
      pre {
        word-break: break-all;
        font-family: inherit;
      }
    }
  }
}


/*奖励*/

.details .award {
  width: 680px;
  height: auto;
  margin-bottom: 40px;
  ul.award-cutBtn {
    list-style: none;
    @include marPad();
    height: 42px;
    width: 100%;
    li.cutBtn-list {
      width: 104px;
      height: 42px;
      float: left;
      line-height: 42px;
      text-align: center;
      font-size: 16px;
      border: 1px solid #e1e1e1;
      border-bottom: none;
      border-radius: 3px 3px 0 0;
      margin-right: 5px;
      cursor: pointer;
      a {
        display: block;
        width: 100%;
        height: 100%;
        color: #666666;
      }
    }
    li.on {
      background: #f8f7f7;
      height: 43px;
      color: $textColor;
    }
  }
  ol.award-cutRect {
    list-style: none;
    @include marPad();
    width: 100%;
    height: auto;
    border: 1px solid #e1e1e1;
    border-radius: 0 3px 3px 3px;
    background: #f8f7f7;
    padding: 19px 33px 31px 33px;
    li.cutRect-list {
      display: none;
    }
    &>li.on {
      display: block;
    }
    .ticket-prevent {
      height: 42px;
      line-height: 42px;
      margin-bottom: 24px;
      border-bottom: 1px solid #e1e1e1;
      color: #fd2f35;
      font-size: 16px;
    }

    .user-case-a {
      height: 42px;
      width: 100%;
      border-bottom: 1px solid #e1e1e1;
      overflow: hidden;
      margin-bottom: 24px;
      span {
        display: block;
        float: left;
        line-height: 42px;
        font-size: 14px;
        color: $textColor;
        margin-right: 31px;
        font-weight: normal;
      }
      em {
        font-style: normal;
        line-height: 37px;
        font-size: 24px;
        color: #fd2f35;
        margin-left: 6px;
      }
    }
    .user-case-b {
      height: 29px;
      overflow: hidden;
      font-weight: normal;
      margin-bottom: 20px;
      span {
        line-height: 34px;
        font-size: 14px;
        color: $textColor;
        float: left;
      }
      em {
        font-style: normal;
        font-size: 24px;
        line-height: 30px;
        color: #fbb03b;
        margin: 0 3px;
      }
      a {
        display: block;
        float: left;
        width: 55px;
        height: 29px;
        color: white;
        background: #fbb03b;
        border: 1px solid #fc9e0b;
        font-size: 16px;
        text-align: center;
        line-height: 28px;
        border-radius: 3px;
        margin-left: 30px;
      }
    }
    /*投票*/
    .vote-box {
      width: 100%;
      height: 139px;
      margin-bottom: 3px;
      p.vote-title {
        margin-bottom: 0;
        height: 40px;
        line-height: 40px;
        font-size: 14px;
        color: $textColor;
      }
      input[type="text"] {
        width: 100%;
        height: 47px;
        padding: 0 10px;
        border: 1px solid #e1e1e1;
        border-radius: 4px;

        &[disabled] {
          color: $text-light;
        }
      }
      a.vote-btn {
        display: block;
        width: 84px;
        height: 32px;
        background: #fd2f35;
        color: white;
        border-radius: 3px;
        font-size: 16px;
        line-height: 32px;
        text-align: center;
        margin: 0 auto;
        margin-top: 20px;
        cursor: pointer;

        &.disabled {
          background-color: #767676;
          cursor: not-allowed;
        }
      }
    }
    /*提示信息*/
    .message {
      font-size: 12px;
      line-height: 20px;
      color: #a6a6a6;
      margin-bottom: 5px;
    }
  }
}


/*投票记录*/

.record {
  width: 614px;
  height: 252px;
  @include marPad();
  dt.record-title {
    height: 46px;
    color: $textColor;
    font-size: 14px;
    line-height: 46px;
    background: bg('white-triangle.png') no-repeat;
    background-position: 24px bottom;
  }
  dd.record-rect {
    @include marPad();
    width: 611px;
    height: 206px;
    overflow: hidden;
    background: white;
    margin-left: 2px;
    border-radius: 5px;
    padding: 25px 0 0 14px;
  }
  div.record-list {
    width: 556px;
    height: 40px;
    overflow: hidden;
    margin-bottom: 22px;
    img {
      display: block;
      float: left;
      width: 30px;
      height: 30px;
      border-radius: 50%;
      margin-right: 13px;
    }
    p {
      @include marPad();
      @include showApo();
      float: left;
      width: 513px;
      font-size: 14px;
      color: #666666;
    }
    a.name {
      float: left;
      min-width: 80px;
      max-width: 260px;
      @include showApo();
      padding-right: 15px;
      color: #666666;
    }
    em {
      font-style: normal;
      line-height: 14px;
    }
    em.time {
      font-size: 10px;
      color: #999999;
      margin-right: 30px;
    }
    em.goods {
      font-size: 12px;
      color: #fd3d42;
    }
  }
}


/*月票部分*/

div.monTicket {
  width: 613px;
  overflow: hidden;
  dl {
    @include marPad();
    position: relative;
    width: 127px;
    height: 51px;
    cursor: pointer;
    float: left;
    margin: 14px 35px 14px 0;
    &:nth-child(4) {
      margin-right: 0;
    }
  }
  &.disabled {
    dd {
      cursor: not-allowed;
      background: bg('monTicket-grey-bg.png') no-repeat;
      p, span {
        color: #909090;
      }
    }
    dl.surpass {
      dd {
        background: bg('monTicket-grey-bg.png') no-repeat;
        em {
          color: #909090;
        }
      }
    }
  }
  dd {
    width: 127px;
    height: 51px;
    @include marPad();
    position: absolute;
    left: 0;
    top: 0;
    padding-left: 18px;
    width: 100%;
    height: 100%;
    background: bg('monTicket-bule-bg.png') no-repeat;
    p {
      @include marPad();
      height: 28px;
      line-height: 28px;
      font-size: 15px;
      color: white;
      overflow: hidden;
    }
    span {
      font-size: 12px;
      line-height: 12px;
      color: white;
    }
  }
  dl.on {
    dt.shade {
      position: absolute;
      left: 0;
      top: 0;
      width: 100%;
      height: 100%;
      background: rgba(0, 0, 0, 0.4);
      z-index: 10;
      border-radius: 10px;
      i {
        position: absolute;
        left: 50%;
        top: 50%;
        margin-left: -10px;
        margin-top: -10px;
        width: 21px;
        height: 21px;
        background: bg('checked.png') no-repeat;
      }
    }
  }
  dl.surpass {
    dd {
      background: bg('monTicket-red-bg.png') no-repeat;
      em {
        font-style: normal;
        font-size: 10px;
        line-height: 32px;
        color: #8e1834;
        float: right;
        margin-right: 2px;
        transform: scale(0.8);
      }
    }
  }
}


/*推荐票*/

div.recommendTicket {
  width: 613px;
  height: 252px;
  padding-top: 16px;
  ul.recommendTicket-rect {
    list-style: none;
    @include marPad();
    height: 95px;
    width: 100%;
    padding-left: 9px;
  }
  li.recommendTicket-list {
    position: relative;
    width: 95px;
    height: 95px;
    float: left;
    cursor: pointer;
    margin-right: 65px;
    img {
      position: absolute;
      left: 5px;
      top: 5px;
      width: 84px;
      height: 84px;
    }
    i {
      font-style: normal;
      position: absolute;
      left: 60px;
      top: 11px;
      width: 58px;
      height: 27px;
      border: 2px solid #fdd442;
      border-radius: 20px;
      font-size: 14px;
      line-height: 25px;
      color: #FBB03B;
      text-align: center;
      background: white;
    }
  }
  li:last-child {
    margin-right: 0;
  }
  li:hover {
    img {
      width: 95px;
      height: 95px;
      left: 0;
      top: 0;
    }
  }
  p {
    @include marPad();
    height: 20px;
    line-height: 20px;
    font-size: 14px;
    color: $textColor;
    margin: 49px 0 15px 0;
    span {
      font-size: 20px;
      color: #fd2f35;
      padding: 0 3px;
    }
  }
  h5 {
    line-height: 20px;
    font-weight: normal;
    font-size: 12px;
    color: #999999;
  }
}


/*打赏*/

div.reward {
  width: 613px;
  height: 340px;
  padding-left: 3px;
  .reward-rect {
    width: 608px;
    height: 278px;
    background: #ffffff;
    border-radius: 4px;
    padding: 16px 0 0 16px;
  }
  dl.reward-list {
    @include marPad();
    position: relative;
    width: 162px;
    height: 130px;
    float: left;
    margin-right: 38px;
    cursor: pointer;
  }
  dl.on {
    dd:first-child {
      z-index: 10;
    }
  }
  dl.reward-list:nth-child(3),
  dl.reward-list:nth-child(6) {
    margin-right: 0;
  }
  dd {
    @include marPad();
    position: absolute;
    left: 0;
    top: 0;
    width: 162px;
    height: 91px;
    padding-top: 9px;
    border-radius: 5px;
    background: white;
    i {
      position: absolute;
      width: 21px;
      height: 21px;
      left: 50%;
      top: 50%;
      margin-left: -10px;
      margin-top: -10px;
      background: bg('checked.png') no-repeat;
    }
    &.shade {
      background: rgba(0, 0, 0, 0.5);
    }
    img {
      display: block;
      margin: auto;
    }
  }
  dt {
    position: absolute;
    left: 0;
    top: 91px;
    width: 162px;
    height: 39px;
    text-align: center;
    line-height: 39px;
    @include showApo();
  }
  .count-ract {
    @include ulEmpty();
    height: 35px;
    margin-top: 22px;
    li {
      float: left;
      line-height: 35px;
      font-size: 14px;
      margin-right: 40px;
    }
    span {
      display: block;
      float: left;
      height: 35px;
      line-height: 35px;
    }
    em.sub,
    em.add {
      font-style: normal;
      display: block;
      float: left;
      width: 24px;
      height: 35px;
      text-align: center;
      line-height: 35px;
      color: #fd2f35;
      font-weight: bold;
      cursor: pointer;
      font-size: 20px;
    }
    input.num {
      float: left;
      width: 35px;
      height: 35px;
      outline: none;
      border: 1px solid #e1e1e1;
      text-align: center;
    }
    em.money {
      font-style: normal;
      display: block;
      float: left;
      color: #fbb03b;
      padding: 0 4px;
    }
  }
}


/*目录*/

.details .catalog {
  width: 680px;
  height: auto;
  margin-bottom: 35px;
  dl.catalog-rect {
    @include marPad();
    width: 100%;
    height: auto;
  }
  dd {
    @include marPad();
    display: none;
    height: auto;
    width: 100%;
  }
  dl.on {
    dd {
      display: block;
    }
  }
  dt.roll {
    position: relative;
    height: 40px;
    font-size: 16px;
    color: $textColor;
    font-weight: bold;
    line-height: 40px;
    padding-left: 34px;
    margin-bottom: 13px;
    background: #f5f5f5;
    cursor: pointer;
    i {
      position: absolute;
      left: 23px;
      top: 12px;
      height: 17px;
      width: 3px;
      background: #fd2f35;
    }
  }
  div.catalog-list {
    width: 100%;
    height: auto;
    overflow: hidden;
    a {
      display: block;
      float: left;
      width: 280px;
      height: 32px;
      line-height: 32px;
      font-size: 14px;
      color: $textColor;
      margin-left: 36px;
      @include showApo();

      &:hover {
        color: #fd2f35;
      }
    }
  }
  p.moreBox {
    @include marPad();
    height: 52px;
    line-height: 52px;
    font-size: 14px;
    padding: 19px 0 0 35px;
    a {
      display: block;
      float: left;
      width: 96px;
      height: 14px;
      line-height: 14px;
      color: #000000;
      background: bg('black-jt.png') no-repeat;
      background-position: right center;
    }
  }
}


/*评论*/

.details .comment-rect {
  width: 100%;
  height: 145px;
  margin-bottom: 33px;
  div.comment-txt {
    width: 100%;
    height: 97px;
    border: 1px solid #e1e1e1;
    border-radius: 5px;
    overflow: hidden;
  }
  textarea {
    width: 100%;
    height: 100%;
    padding: 5px 10px;
    outline: none;
    resize: none;
    border: none;
  }
  .comment-btn {
    display: block;
    width: 103px;
    height: 32px;
    text-align: center;
    line-height: 32px;
    font-size: 15px;
    color: white;
    background: #FD2F35;
    border-radius: 3px;
    margin-top: 16px;
    cursor: pointer;
  }
}


/*评论列表*/

.details .commentList-rect {
  width: 100%;
  height: auto;
  .comment-box {
    height: auto;
    width: 100%;
    border: 1px solid #e1e1e1;
    border-radius: 3px;
    padding-left: 19px;
  }
  /*每一条回复*/
  dl.comment-list {
    @include marPad();
    margin-top: 31px;
    height: auto;
    overflow: hidden;
    margin-bottom: 30px;
  }
  dt.list-tx {
    height: 100%;
    width: 40px;
    float: left;
    margin-right: 14px;
    img {
      width: 40px;
      height: 40px;
      border-radius: 50%;
    }
  }
  dd {
    @include marPad();
    float: left;
    width: 583px;
    height: auto;
    border-bottom: 1px solid #f2f2f2;
    padding-bottom: 20px;
    h2 {
      @include marPad();
      height: 17px;
      line-height: 17px;
      font-size: 13px;
      color: #999999;
      margin-bottom: 4px;
      a {
        color: #fbb548;
        font-weight: normal;
        padding: 0 4px;
      }
    }
    p.con-sent {
      @include marPad();
      line-height: 24px;
      font-size: 14px;
      color: #666666;
      margin-bottom: 19px;
      word-break: break-all;
    }
    p.reply-sent{
      line-height: 24px;
      font-size: 14px;
      background: #f5f5f5;
      padding: 13px 16px 10px 16px;
      border-radius: 4px;
    }
    .reply-rect{
      height: auto;
      margin-top: 16px;
      position: relative;
      background: #fff;
      textarea {
        width: 100%;
        height: 79px;
        border: 1px solid #e1e1e1;
        border-radius: 3px;
        outline: none;
        resize:none;
        padding: 5px 10px;
      }

      .send-btn{
        display: inline-block;
        width: 89px;
        height: 29px;
        background: #fd2f35;
        color: #fff;
        font-size: 16px;
        text-align: center;
        border-radius: 3px;
        cursor: pointer;
        margin-top: 5px;
        border: 0 none;
      }

      &::after,&::before{
        position: absolute;
        right: 40px;
        top: -9px;
        content: '';
        width: 0;
        height: 0;
        border: 5px solid transparent;
        border-bottom-color: #fff;
      }
      &::before{
        top: -10px;
        border-bottom-color: #e1e1e1;
      }

      &.reply-of-reply {
        &::after,&::before {
          right: 10px;
        }
      }
    }
    .replyOfReply{
      height: auto;
      background: #f5f5f5;
      border-radius: 3px;
      position: relative;
      margin-top: 22px;
      padding: 15px 20px 30px 17px;
      &::after{
        position: absolute;
        left: 36px;
        top: -16px;
        content: '';
        width: 0;
        height: 0;
        border: 8px solid transparent;
        border-bottom-color: #f5f5f5;
      }
      .reply-list{
        overflow: hidden;
        .userPortrait{
          width: 35px;
          height: 35px;
          border-radius: 50%;
          float: left;
          margin-right: 14px;
          margin-bottom: 12px;
        }
        .userName{
          float: left;
          line-height: 35px;
          font-size: 14px;
        }
        .reply-sent{
          @include marPad();
          clear: both;
          margin-bottom: 9px;
        }
        .reply {
          line-height: 14px;
          font-style: normal;
          cursor: pointer;
          float: right;
          color: #9f9f9f;
        }
        .praise{
          margin-left: 40px;
          height: 14px;
          font-style: normal;
          cursor: pointer;
          float: right;
          i{
            float: left;
            width: 15px;
            height: 13px;
            margin-right: 3px;
          }
          i.no-heart {
            background: bg('no-heart.png') no-repeat;
          }
          i.heart{
            background: bg('heart.png') no-repeat;
          }
          span{
            float: left;
            line-height: 14px;
            color: #9f9f9f;
          }
        }
      }
    }
    > ul {
      list-style: none;
      @include marPad();
      height: 20px;
      overflow: hidden;
      li {
        float: left;
        span.time {
          float: left;
          font-size: 11px;
          line-height: 29px;
          color: #999999;
        }
        em.reply,
        em.praise {
          font-style: normal;
          float: left;
          font-size: 13px;
          line-height: 27px;
          color: #999999;
          cursor: pointer;
        }
        em.reply{
          margin-right: 30px;
        }
        i.ding {
          float: left;
          height: 19px;
          margin-left: 9px;
          width: 23px;
          background: bg('ding.png') no-repeat;
        }
        i.jing {
          float: left;
          height: 19px;
          margin-left: 9px;
          width: 19px;
          background: bg('jing.png') no-repeat;
        }
        em.praise {
          i{
            float: right;
            margin: 7px 7px 0 0;
            width: 15px;
            height: 13px;
          }
          i.no-heart {
            background: bg('no-heart.png') no-repeat;
          }
          i.heart{
            background: bg('heart.png') no-repeat;
          }
          span {
            float: right;
          }
        }
      }
      li:nth-child(2) {
        float: right;
      }
    }
    
  }
}


